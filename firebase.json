{
  "hosting": [
		{

			"target": "material-hosting",
			"public": "dist/apps/materialapp",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		},
		{
