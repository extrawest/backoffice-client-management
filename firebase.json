{
  "hosting": [
		{

			"target": "material-hosting",
			"public": "dist/apps/material",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		},
		{

			"public": "dist/apps/tailwind ",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "/tailwind/**",
					"destination": "/tailwind/index.html"
				}
			]
		}
	]
}
