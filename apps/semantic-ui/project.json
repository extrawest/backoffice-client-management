{
	"name": "semantic-ui",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/semantic-ui/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/webpack:webpack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"compiler": "babel",
				"outputPath": "dist/apps/semantic-ui",
				"index": "apps/semantic-ui/src/index.html",
				"baseHref": "/",
				"main": "apps/semantic-ui/src/main.tsx",
				"polyfills": "apps/semantic-ui/src/polyfills.ts",
				"tsConfig": "apps/semantic-ui/tsconfig.app.json",
				"assets": [
					"apps/semantic-ui/src/favicon.ico",
					"apps/semantic-ui/src/assets"
				],
				"styles": ["apps/semantic-ui/src/styles.scss"],
				"scripts": [],
				"webpackConfig": "@nrwl/react/plugins/webpack"
			},
			"configurations": {
				"development": {
					"extractLicenses": false,
					"optimization": false,
					"sourceMap": true,
					"vendorChunk": true
				},
				"production": {
					"fileReplacements": [
						{
							"replace": "apps/semantic-ui/src/environments/environment.ts",
							"with": "apps/semantic-ui/src/environments/environment.prod.ts"
						}
					],
					"optimization": true,
					"outputHashing": "all",
					"sourceMap": false,
					"namedChunks": false,
					"extractLicenses": true,
					"vendorChunk": false
				}
			}
		},
		"serve": {
			"executor": "@nrwl/webpack:dev-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "semantic-ui:build",
				"hmr": true
			},
			"configurations": {
				"development": {
					"buildTarget": "semantic-ui:build:development"
				},
				"production": {
					"buildTarget": "semantic-ui:build:production",
					"hmr": false
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/semantic-ui/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/semantic-ui/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
